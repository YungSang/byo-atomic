{
    "comment": "Default/demo trees generated for CentOS Atomic",

    "osname": "centos-atomic",
    "ref": "centos-atomic/7/x86_64/base",
    
    "repos": ["base", "updates," "extras," "centosplus," "walters-rpm-ostree", "epel", "maxamillion-epel7-cockpit"],

    "selinux": true,

    "bootstrap_packages": ["filesystem", "glibc", "nss-altfiles", "shadow-utils",
			   "centos-release"],

    "packages": ["kernel", "rpm-ostree", "lvm2", "syslinux-extlinux",
		 "btrfs-progs", "e2fsprogs", "xfsprogs",
		 "docker",
		 "selinux-policy-targeted",
		 "audit",
		 "sssd",
		 "cloud-init",
		 "tar",
		 "openssh-server", "openssh-clients",
		 "passwd",
		 "NetworkManager", "vim-minimal", "nano",
		 "sudo"],

    "units": ["docker.service"],

    "default_target": "multi-user.target"
}
